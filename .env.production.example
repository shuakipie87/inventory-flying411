# Flying411 Production Environment Template
# Copy to .env and fill in values

# Database
DB_USER=flying411
DB_PASSWORD=your_secure_password_here
DB_NAME=flying411

# Redis
REDIS_PASSWORD=your_redis_password_here

# JWT Authentication
JWT_SECRET=your_very_long_secret_key_at_least_32_chars
JWT_EXPIRES_IN=7d

# URLs
FRONTEND_URL=https://flying411.com
BACKEND_URL=https://api.flying411.com

# Flying411 Integration
FLYING411_API_URL=https://api.flying411.com/v1
FLYING411_API_KEY=your_flying411_api_key
FLYING411_WEBHOOK_SECRET=your_webhook_secret

# SSL Certificate
SSL_EMAIL=admin@flying411.com

# Optional: External Services
# SENTRY_DSN=https://key@sentry.io/project
# AWS_S3_BUCKET=flying411-uploads
# AWS_ACCESS_KEY_ID=your_aws_key
# AWS_SECRET_ACCESS_KEY=your_aws_secret

# Unsplash API
UNSPLASH_ACCESS_KEY=your-unsplash-access-key-here
UNSPLASH_SECRET_KEY=your-unsplash-secret-key-here

# Gemini AI
GEMINI_API_KEY=your-gemini-api-key-here
